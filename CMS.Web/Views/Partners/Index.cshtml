@model List<CMS.Core.Entity.Partners>
@{
    ViewData["Title"] = "Partners";
}
<section>
    <div class="bredcrumb-section padTB100 positionR">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="theme-heading text-center">
                        <h3 class="Breadcrumb-title text-center">Partners</h3>
                        <span class="colorW marB10"> <i class="fa fa-angle-down" aria-hidden="true"></i> </span>
                    </div>
                    <div class="breadcrumb-box">
                        <ul class="breadcrumb text-center colorW marB0">
                            <li>
                                <a href="/">home</a>
                            </li>
                            <li>Partners</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<div class="lecturers-page1-area">
    <div class="container">
        <div class="row">
            @{
                foreach (var partners in Model)
                {

                    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12" style="border:1px solid green; border-radius:15px; margin:15px; margin-right:80px; padding:15px;">
                        <div class="single-item">
                            <div class="lecturers1-item-wrapper">
                                <div class="lecturers-img-wrapper">
                                    @{
                                        var imgsSrc = "/images/";
                                        if (string.IsNullOrWhiteSpace(partners.logo_name))
                                        {
                                            imgsSrc = "/img/user.png";
                                        }
                                        else
                                        {
                                            imgsSrc += "custom/" + partners.logo_name;

                                        }
                                    }
                                    <a><img class="img-responsive" src="@imgsSrc" alt="team" style="height:300px;width:280px;"></a>
                                </div>
                                <div class="lecturers-content-wrapper">
                                    <h3 class="item-title" style="margin-left:80px;"><a href="">@partners.name.ToUpperInvariant()</a></h3>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>
    </div>
</div>







