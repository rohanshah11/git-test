@using Microsoft.AspNetCore.Mvc.Localization
@inject IHtmlLocalizer<SharedResource> Localizer

@{
    var setups = (List<CMS.Core.Entity.Setup>)ViewBag.setup;

    var OpeninngHours = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getOpeningHours).SingleOrDefault()?.value;
    var days = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getDays).SingleOrDefault()?.value;
    var hours = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getHours).SingleOrDefault()?.value;
    var contactNo = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getPhoneNumberKey).SingleOrDefault()?.value;
    var email = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getEmailKey).SingleOrDefault()?.value;

    var address = setups.Where(a => a.key == CMS.Web.Models.SetupKeys.getAddressKey).SingleOrDefault()?.value;
}
@model CMS.Core.Dto.AppointmentDto


<div class="clear"></div>
<!--//================Slider start==============//-->
<section id="slider-section">

    <div id="main-slider" class="owl-carousel owl-theme slider positionR">
        @{
            var ssn = 1;
            foreach (var slider in ViewBag.sliderImages)
            {
                var desc = "";
                if (slider.description != null)
                {
                    if (slider.description.Length > 60)
                    {
                        desc = slider.description.Substring(0, 60);
                    }
                    else
                    {
                        desc = slider.description;
                    }
                }
                if (@slider.image_name != null)
                {
                    <div class="item positionR">

                        <figure class="slider-image positionR">

                            <img src="/images/custom/@slider.image_name" alt="" class="hidden-xs" />
                            <img src="/images/custom/@slider.image_name" alt="" class="hidden-sm hidden-lg hidden-md" />

                        </figure>

                        <div class="slider-text positionA text-center">
                            <div class="container">
                                <div class="row">
                                    <div class="col-md-8 col-sm-8 col-xs-10 col-md-offset-2 col-sm-offset-2 col-xs-offset-1 text-center">
                                        <h1><span class="theme-color">@Html.Raw(slider.title)</span></h1>
                                        <p>
                                            @desc
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                }
                ssn++;
            }
        }
    </div>

</section>
