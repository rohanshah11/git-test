<section class="padT100 padB100">
    <!--- Theme heading start-->
    <div class="theme-heading marB30 positionR">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-6 col-xs-10  col-md-offset-4 col-sm-offset-3 col-xs-offset-1 heading-box text-center">
                    <h3 class="marB10">Testimonial</h3>
                    <span class="theme-color marB10"> <i class="fa fa-angle-down" aria-hidden="true"></i> </span>
                    <p class=" marB10">
                    </p>
                </div>
            </div>
        </div>
    </div>
    <!--- Theme heading end-->
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-sm-10 col-xs-12 col-md-offset-2 col-sm-offset-1 col-xs-12 col-xs-offset-0">
                <div id="testimonial" class="owl-carousel owl-theme sliders testimonial-carousel">
                    @{
                        var test = 1;
                        foreach (var testimonialSlider in ViewBag.testimonial)
                        {
                            if (testimonialSlider != null)
                            {
                                var testimonialClass = "";
                                if (test == 1)
                                {
                                    testimonialClass = "active";
                                }
                                var imgSrc = "/images/";
                                if (string.IsNullOrWhiteSpace(testimonialSlider.image_name))
                                {
                                    imgSrc += "default-image.jpg";
                                }
                                else
                                {
                                    imgSrc += "custom/" + testimonialSlider.image_name;

                                }
                                <div class="client-col text-center">
                                    <div class="frame">
                                        <img src="@imgSrc" alt="" class="img-circle">
                                    </div>
                                    <div class="text-box">
                                        <strong class="name">@testimonialSlider.person_name</strong>
                                    </div>
                                    <p>
                                        @testimonialSlider.statement
                                    </p>
                                    <div class="text-box">
                                        <span class="theme-color">@testimonialSlider.designation</span>
                                    </div>
                                </div>
                                test++;
                            }
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<!--//================Testimonial end==============//-->
