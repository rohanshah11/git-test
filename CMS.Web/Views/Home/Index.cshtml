                                imgSrc += "default-image.jpg";
                            }
                            else
                            {
                                imgSrc += "custom/" + part.logo_name;

                            }
                            <div class="item">
                                <figure class="border-box">
                                    <a href="//@part.web_url" target="_blank">
                                        <img src="@imgSrc" alt="" style="height:150px;width:260px;">
                                    </a>
                                </figure>
                            </div>
                        }

                    }

                </div>

            </div>
        </div>
    </div>
</section>